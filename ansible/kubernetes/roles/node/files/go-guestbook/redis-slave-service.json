{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "redis-slave",
        "namespace": "guestbook",
        "labels": {
            "app": "redis",
            "role": "slave"
        },
        "annotations": {
            "com.illumio.role": "REDIS-WORKER-SVC"
        }
    },
    "spec": {
        "ports": [
            {
                "port": 6379,
                "targetPort": "redis-server"
            }
        ],
        "selector": {
            "app": "redis",
            "role": "slave"
        }
    }
}